{
  "name": "glaze_dental",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 6001",
    "build": "npm run generate-image-sitemap && next build && npm run copy-htaccess && npm run copy-image-sitemap",
    "copy-htaccess": "cp public/.htaccess out/.htaccess",
    "copy-image-sitemap": "cp public/sitemap-images.xml out/sitemap-images.xml",
    "generate-image-sitemap": "node scripts/generate-image-sitemap.js",
    "start": "next start",
    "lint": "eslint"
  },
  "dependencies": {
    "next": "16.0.10",
    "react": "19.2.1",
    "react-dom": "19.2.1"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.0.10",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
